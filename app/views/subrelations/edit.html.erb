<h1>Editing subrelation</h1>

<% form_for(@subrelation) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :section_id %><br />
    <%= f.text_field :section_id %>
  </p>
  <p>
    <%= f.label :subsection_id %><br />
    <%= f.text_field :subsection_id %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @subrelation %> |
<%= link_to 'Back', subrelations_path %>